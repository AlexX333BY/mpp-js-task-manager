<tr class="<%= taskEntryClass %>">
    <td class="task-name task-spec"><%= taskName %></td>
    <td class="task-spec"><%= expectedCompleteDate %></td>
    <td class="task-spec"><%= taskStatus %></td>
    <td class="task-spec">
        <form method="get" action="/downloadTaskAttachment">
            <input type="submit" value="<%= downloadAttachment %>" <%= downloadAttachmentDisabled %> />
            <input type="hidden" name="taskId" value="<%= taskId %>" />
        </form>
    </td>
    <td class="task-spec">
        <input type="button" value="<%= completeTask %>" <%= completeTaskDisabled %> onclick="completeTaskAndUpdateTasksAsync(<%= taskId %>)"/>
    </td>
</tr>
